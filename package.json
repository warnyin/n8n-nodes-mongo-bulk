{
  "name": "@warnyin/n8n-nodes-mongo-bulk",
  "version": "1.0.8",
  "description": "n8n node for MongoDB bulk and many operations",
  "license": "MIT",
  "author": {
    "name": "thebossza101"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/warnyin/n8n-nodes-mongo-bulk.git"
  },
  "bugs": {
    "url": "https://github.com/warnyin/n8n-nodes-mongo-bulk/issues"
  },
  "homepage": "https://github.com/warnyin/n8n-nodes-mongo-bulk#readme",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "mongodb",
    "bulk",
    "bulk operations",
    "insertMany",
    "updateMany",
    "deleteMany"
  ],
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy",
    "copy": "copyfiles -u 0 \"nodes/**/*.{png,svg,json}\" dist/",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\"",
    "lintfix": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "assets",
    "README.md",
    "LICENSE.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/MongoDbBulk/MongoDbBulk.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "n8n-workflow": "^1.0.0",
    "typescript": "^5.1.3",
    "copyfiles": "^2.4.1",
    "prettier": "^2.8.8"
  },
  "dependencies": {
    "mongodb": "^6.3.0"
  }
}
